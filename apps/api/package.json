{
  "name": "fbif-form-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "test": "tsx --test test/**/*.test.ts",
    "test:coverage": "c8 --check-coverage --lines 80 --functions 80 --branches 45 --statements 80 --reporter=text --reporter=lcov npm run test",
    "start": "node dist/index.js",
    "worker": "tsx src/worker.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.22.0",
    "@prisma/client": "^5.19.1",
    "bullmq": "^5.16.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "pino": "^9.3.2",
    "pino-http": "^9.0.0",
    "rate-limit-redis": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/cors": "^2.8.17",
    "@types/supertest": "^6.0.3",
    "prisma": "^5.19.1",
    "supertest": "^7.1.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
